{
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react",
    "jsxFactory": "stick.jsx",
    "jsxFragmentFactory": "stick.Fragment",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "strict": true,
    "esModuleInterop": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  }
}